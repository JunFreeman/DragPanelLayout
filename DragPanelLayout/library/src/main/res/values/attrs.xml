<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="DragPanelLayout">
        <attr name="kdplDraggableView" format="reference"/>
        <attr name="kdplDragBarHeight" format="dimension|reference"/>
        <attr name="kdplParallaxOffset" format="dimension|reference"/>
        <attr name="kdplForbiddenZoneSize" format="dimension|reference"/>
        <attr name="kdplContentDragEnabled" format="boolean|reference"/>
        <attr name="kdplPanelState" format="enum">
            <enum name="collapsed" value="0"/>
            <enum name="expanded" value="1"/>
            <enum name="hidden" value="2"/>
            <enum name="dragging" value="3"/>
            <enum name="settling" value="4"/>
        </attr>
        <attr name="kdplPanelLayoutType" format="enum">
            <enum name="overlayContent" value="0"/>
            <enum name="alignEdge" value="1"/>
            <enum name="followContent" value="2"/>
        </attr>
    </declare-styleable>

</resources>